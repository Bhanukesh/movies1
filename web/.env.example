# Frontend Environment Variables
# Copy this file to .env.local for local development

# API URLs - These will be automatically configured when using Aspire
NEXT_PUBLIC_DOTNET_API_URL=https://localhost:7001
NEXT_PUBLIC_PYTHON_API_URL=http://localhost:8000

# Development tools
NEXT_PUBLIC_ENABLE_REDUX_DEV_TOOLS=true

# For production, set NEXT_PUBLIC_ENABLE_REDUX_DEV_TOOLS=false